###Hydra AEP TE analysis

wget https://api.ncbi.nlm.nih.gov/datasets/v2/genome/accession/GCF_038396675.1/download?include_annotation_type=GENOME_FASTA&include_annotation_type=GENOME_GFF&include_annotation_type=RNA_FASTA&include_annotation_type=CDS_FASTA&include_annotation_type=PROT_FASTA&include_annotation_type=SEQUENCE_REPORT&hydrated=FULLY_HYDRATED

nano repeatmodeler_hydra_AEP
#!/bin/bash
#SBATCH -p 128x24   # Partition name
#SBATCH -J repeatmodeler_hydra_AEP        # Job name
#SBATCH --mail-user=aymays@ucsc.edu
#SBATCH --mail-type=ALL
#SBATCH -o repeatmodeler_hydraAEP.out    # Name of stdout output file
#SBATCH -N 1       # Total number of nodes requested (128x24/Instructional only)
#SBATCH -n 16        # Total number of mpi tasks requested per node
#SBATCH -t 64:00:00  # Run Time (hh:mm:ss) - 1.5 hours (optional)
#SBATCH --mem=12G # Memory to be allocated PER NODE

export LC_ALL='C'

module load repeatmodeler

BuildDatabase -name hydra_AEP_db hydra_AEP.fna

RepeatModeler -engine ncbi -threads 16 -database hydra_AEP_db
